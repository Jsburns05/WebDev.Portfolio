play: flex;\r\n    flex-direction: column;\r\n    height: 100%;\r\n    width: 100%;\r\n}\r\n\r\n/*-----------------------------------------------------\r\n\r\n\r\n\r\n\r\n\r\nHEADER AND NAV LAYOUT\r\n-----------------------------------------------------*/\r\n\r\n.header{\r\n    color: var(--clr-primary);\r\n    grid-column: 1 / 2;\r\n    grid-row: 1 / 2;\r\n    width: 100%;\r\n    height: 100%;\r\n}\r\n\r\n.navbar {\r\n    display: flex;\r\n    justify-content: space-between;\r\n    width: 100%;\r\n}\r\n\r\n/*\r\nHEADER AND NAV STYLING\r\n-----------------------------------------------------*/\r\n.header {\r\n    background: var(--clr-background-primary);\r\n\r\n}\r\n\r\n.nav-logo {\r\n    color: var(--clr-accent);\r\n    font-family: var(--ff-special);\r\n    font-size: 2rem;\r\n    margin: 1rem;\r\n\r\n}\r\n\r\n.nav-link {\r\n    color: var(--clr-primary);\r\n    cursor: pointer;\r\n    font-weight: 200;\r\n    font-size: 1.3rem;\r\n    padding: 0 1rem;\r\n}\r\n\r\n.nav-link:hover {\r\n    color: var(--clr-accent);\r\n}\r\n\r\n.nav-menu {\r\n    display: flex;\r\n}\r\n\r\n/*-----------------------------------------------------\r\n\r\n\r\n\r\n\r\n\r\nABOUT FEATURE SECTION START\r\n-----------------------------------------------------*/\r\n\r\n.about-feature {\r\n    display: flex;\r\n    flex-direction: column;\r\n    align-items: center;\r\n    margin-top: 7rem;\r\n    width: 100%;\r\n}\r\n\r\n.about-feature img {\r\n    width: 12rem;\r\n    border: 2px solid var(--clr-primary);\r\n    border-radius: 5px;\r\n}\r\n\r\n.about-box {\r\n    display: flex;\r\n    flex-direction: column;\r\n    width: 300px;\r\n    height: 220px;\r\n    background: var(--clr-accent);\r\n    border-radius: 5px;\r\n    position: relative;\r\n    top: -4rem;\r\n    z-index: -1;\r\n    outline: 2px solid var(--clr-primary);\r\n    outline-offset: 5px;\r\n}\r\n\r\n.about-box h1 {\r\n    font-family: var(--ff-special);\r\n    font-size: 1.8rem;\r\n    padding-top: 4rem;\r\n    margin-bottom: .6rem;\r\n    color: var(--clr-background-primary);\r\n    letter-spacing: .05rem;\r\n    font-weight: 200;\r\n}\r\n\r\n.about-box h2 {\r\n    font-size: 1.3rem;\r\n    margin: 0;\r\n    color: var(--clr-primary);\r\n    font-weight: 100;\r\n}\r\n\r\n/*-----------------------------------------------------\r\n\r\n\r\n\r\n\r\n\r\nMAIN SECTION LAYOUT\r\n-----------------------------------------------------*/\r\n\r\n.main {\r\n    margin-top: 5rem;\r\n}\r\n\r\n.page-titles {\r\n    font-family: var(--ff-special);\r\n    color: var(--clr-accent);\r\n    font-size: 2rem;\r\n    margin-top: 0;\r\n}\r\n\r\n.card-text-primary {\r\n    position: relative;\r\n    top: -1rem;\r\n    z-index: 1;\r\n    font-size: 1.5rem;\r\n    font-family: var(--ff-special);\r\n    color: var(--clr-accent);\r\n    margin: 0;\r\n    padding: 0 2rem;\r\n}\r\n\r\n.card-text-secondary {\r\n    position: relative;\r\n    top: -1rem;\r\n    font-size: 1.5rem;\r\n    font-family: var(--ff-special);\r\n    color: var(--clr-background-secondary);\r\n    margin: 0;\r\n    padding: 0 2rem;\r\n}\r\n\r\n.main-container {\r\n    display: flex;\r\n    flex-direction: column;\r\n    padding: 0 5rem;\r\n}\r\n\r\n.main-item {\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content: center;\r\n    background: var(--clr-background-secondary);\r\n    color: var(--clr-primary);\r\n    cursor: pointer;\r\n    border-radius: 5px;\r\n    outline: 2px solid var(--clr-accent);\r\n    outline-offset: 5px;\r\n    margin: 2rem;\r\n    height: 22rem;\r\n    transition: .3s ease-in-out;\r\n}\r\n\r\n.main-item:hover {\r\n    background: var(--clr-accent);\r\n    outline: 2px solid var(--clr-primary);\r\n    outline-offset: 5px;\r\n    transition: .4s ease-in-out;\r\n}\r\n\r\n\r\n/*-----------------------------------------------------\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\nMAIN SECTION BREAKPOINT\r\n-----------------------------------------------------*/\r\n\r\n@media screen and (min-width:768px) {\r\n    .main {\r\n        grid-column: 1 / 4;\r\n    }\r\n\r\n    .main-container img {\r\n        height: 250px;\r\n        max-width: 100%;\r\n    }\r\n\r\n    .main-container {\r\n        display: grid;\r\n        grid-template-columns: 1fr 1fr;\r\n        max-width: 100%;\r\n    }\r\n\r\n}\r\n\r\n@media screen and (min-width: 1024px) {\r\n    .main-container {\r\n        display: grid;\r\n        grid-template-columns: 1fr 1fr 1fr;\r\n        margin-top: 1.5rem;\r\n    }\r\n\r\n    .main-container img {\r\n        max-width: 100%;\r\n        height: 250px;\r\n\r\n    }\r\n}\r\n\r\n/*-----------------------------------------------------\r\n\r\n\r\n\r\n\r\n\r\nFOOTER SECTION LAYOUT\r\n-----------------------------------------------------*/\r\n\r\n.footer {\r\n    grid-column: 1 / 3;\r\n}\r\n\r\n@media screen and (min-width: 768px) {\r\n    .footer {\r\n        grid-column: 1 / 4;\r\n    }\r\n}\r\n\r\n/*\r\nFOOTER SECTION STYLING\r\n-----------------------------------------------------*/\r\n\r\n.footer {\r\n    color: var(--clr-primary);\r\n    border: 2px solid var(--clr-accent);\r\n}\r\n\r\n\r\n\r\n\r\n\r\n\r\n\r\n"},"lineCount":null}},"error":null,"hash":"83f8e0452852040868e4e58702fd6226","cacheData":{"env":{}}}