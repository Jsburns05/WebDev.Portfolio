-primary: 'Roboto', sans-serif;\r\n    --ff-special: 'Russo One', sans-serif;\r\n    --clr-background-primary: rgb(34, 34, 34);\r\n    --clr-background-secondary: rgb(55, 55, 55);\r\n    --clr-primary: rgb(255, 255, 255);\r\n    --clr-accent: rgb(92, 152, 236);\r\n}\r\n\r\nbody {\r\n    background-color: var(--clr-background-primary);\r\n    font-family: var(--ff-primary);\r\n    font-size: 1.5rem;\r\n    height: 100vh;\r\n    letter-spacing: .1em;\r\n    text-align: center;\r\n}\r\n\r\nli {\r\n    list-style: none;\r\n}\r\n\r\na {\r\n    text-decoration: none;\r\n}\r\n\r\n/*-----------------------------------------------------\r\n\r\n\r\n\r\n\r\n\r\nBREAKPOINT GRID SETUP\r\n-----------------------------------------------------*/\r\n\r\n.grid-container {\r\n    display: flex;\r\n    flex-direction: column;\r\n    height: 100%;\r\n}\r\n\r\n@media screen and (min-width: 768px) {\r\n    .grid-container {\r\n        display: grid;\r\n\r\n    }\r\n}\r\n\r\n@media screen and (min-width: 1024px) {\r\n    .grid-container {\r\n        display: grid;\r\n        grid-template-columns: 1fr 1fr 1fr;\r\n    }\r\n\r\n    \r\n}\r\n\r\n/*-----------------------------------------------------\r\n\r\n\r\n\r\n\r\n\r\nHEADER AND NAV LAYOUT\r\n-----------------------------------------------------*/\r\n\r\n.header{\r\n    color: var(--clr-primary);\r\n    grid-column: 1 / 2;\r\n    grid-row: 1 / 2;\r\n    display: flex;\r\n    flex-direction: row;\r\n    width: 100%;\r\n    height: 100%;\r\n}\r\n\r\n.navbar {\r\n    display: flex;\r\n    justify-content: space-between;\r\n    width: 100%;\r\n    padding-top: 1rem;\r\n}\r\n\r\n/*\r\nHEADER AND NAV STYLING\r\n-----------------------------------------------------*/\r\n.header {\r\n    background: var(--clr-background-primary);\r\n\r\n}\r\n\r\n.nav-logo {\r\n    color: var(--clr-accent);\r\n    font-family: var(--ff-special);\r\n    font-size: 2rem;\r\n    margin: 0 0 1rem 1.6rem;\r\n}\r\n\r\n.nav-link {\r\n    color: var(--clr-primary);\r\n  